{
    "name": "Tippekonkurranse",
    "version": "1.3.10",
    "description": "An idiomatic Node-based web application hosted on Heroku",
    "author": "Eirik Torske <eirik.torske@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/eirikt/tippekonkurranse.git"
    },
    "dependencies": {
        "mongodb": "2.x",
        "mongoose": "4.x",
        "express": "4.x",
        "underscore": "1.x",
        "scoreunder": "0.x",
        "moment": "2.x",
        "async-rq": "0.x",
        "ramda": "0.x",
        "request": "2.x",
        "rq-essentials": "eirikt/rq-essentials",
        "iconv-lite": "0.x",
        "cheerio": "0.x",
        "logdown": "1.x",

        "phantomjs": "2.x",
        "mocha-phantomjs": "4.x",
        "chai": "3.x",
        "chai-jquery": "2.x",
        "sinon": "1.x",
        "bower": "1.x",
        "grunt": "0.x",
        "grunt-cli": "0.x",
        "grunt-env": "0.x",
        "grunt-shell": "1.x",
        "grunt-mocha": "0.x",
        "grunt-mocha-test": "0.x",
        "grunt-mocha-cov": "0.x",
        "grunt-blanket-mocha": "0.x",
        "grunt-saucelabs": "8.x",
        "grunt-jsdoc": "1.x",
        "grunt-contrib-clean": "1.x",
        "grunt-contrib-connect": "1.x",
        "grunt-contrib-copy": "1.x",
        "grunt-contrib-cssmin": "1.x",
        "grunt-contrib-jshint": "1.x",
        "grunt-contrib-uglify": "1.x",
        "grunt-contrib-watch": "1.x",
        "proxyquire": "1.x",
        "time-grunt": "1.x"
    },
    "scripts": {
        "postinstall": "echo postinstall time; ./node_modules/grunt-cli/bin/grunt deploy:heroku",
        "start": "node server/scripts/server.js",
        "test": "grunt test",
        "run": "grunt run"
    },
    "config": {
        "blanket": {
            "pattern": [
                "server/scripts"
